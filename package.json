{
  "name": "experiment-learning-platform",
  "version": "1.0.0",
  "description": "Interactive Experiment Learning Platform",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
